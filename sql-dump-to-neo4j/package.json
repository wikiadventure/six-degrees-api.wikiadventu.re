{
  "name": "sql-dump-to-neo4j",
  "module": "index.ts",
  "devDependencies": {
    "@types/bun": "^1.2.16",
    "@types/node": "^20.12.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "ts-node": "node --max-old-space-size=8192 --env-file=.env --loader ts-node/esm",
    "start": "node --max-old-space-size=10240 --loader ts-node/esm index.ts",
    "bun": "bun run index.ts",
    "start-GCP": "node --max-old-space-size=10240 --loader ts-node/esm indexGCP.ts",
    "bun-GCP": "bun run indexGCP.ts"
  },
  "type": "module",
  "dependencies": {
    "large-map": "^1.0.0",
    "neo4j-driver": "^5.28.1",
    "zod": "^3.25.64"
  }
}
