    "workspaceFolder": "/project",
    "build": {
        "dockerfile": "./Dockerfile.dev",
        "context": ".."
    },
    "customizations": {
        "vscode": {
            "extensions": [
				"bmewburn.vscode-intelephense-client",
				"eamodio.gitlens",
